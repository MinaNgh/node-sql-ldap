	<title>Get Report</title>
</head>
<body>
	<!-- <div class="loader" id="processing" style="display:none"></div> -->
	<div class="container" id="report-div">
		<% if (message.length>0) { %>
		<div class="successMessage flash alert alert-<%=type%> "><%= message %></div>
		<!-- <div class="alert alert-danger flash"><%= type %></div> -->
		<% } %>

		<div id="processing">
		  <div id="block_1" class="barlittle"></div>
<div id="block_2" class="barlittle"></div>
<div id="block_3" class="barlittle"></div>
<div id="block_4" class="barlittle"></div>
<div id="block_5" class="barlittle"></div>
</div>
		<!-- action="/report"  -->
		<form  id="reportForm" >

		  <div class="form-group">
		    <label for="startDate">Start Date</label>
		    <input type="date" class="form-control" id="startDate" aria-describedby="startDateHelp" placeholder="Enter Strat Date" name="startDate" required="required" maxlength="10">
		    <!-- <small id="startDateHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> -->
		  </div>
		  <div class="form-group">
		    <label for="endDate" >End Date</label>
		    <input type="date" class="form-control" id="endDate" placeholder="Enter End Date" name="endDate" required="required" maxlength="10">

		  </div>
		  <div class="form-group">
			<label for="type">Type</label>
			<select class="form-control" id="type" name="type" required="required">
				<option value="">Select type...</option>
				<option value="INV">INV</option>
				<option value="SO">SO</option>
				<option value="CM-R">CM-R</option>				
			</select>
		  </div>
		   <div class="form-group">
		  <!-- <input type="submit" class="btn btn-primary" value="Download" id="download-btn"></input> -->
		  <button type="submit" class="btn btn-primary " value="Download" id="download-btn">Download</button>

		</div>
		  <!-- <span class="loader" id="processing" style="display:none"></span> -->
		  
<img id="idaptiveLogo" src="./img/idaptive.jpeg" alt="">

		</form>
		
	</div>
